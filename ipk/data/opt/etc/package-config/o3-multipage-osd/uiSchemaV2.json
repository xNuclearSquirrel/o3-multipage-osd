        {
          "type": "row",
          "items": ["dropdown1", "dropdown2"]
        },
        {
          "type": "row",
          "items": ["checkboxes"]
        }
      ],
      "dropdown1": {
        "ui:widget": "select",
        "ui:options": {
          "label": false
        }
      },
      "dropdown2": {
        "ui:widget": "select",
        "ui:options": {
          "label": false
        }
      },
      "checkboxes": {
        "ui:widget": "checkboxes",
        "classNames": "inline-checkboxes",
        "ui:options": {
          "inline": true
        }
      }
    }
  }
}
